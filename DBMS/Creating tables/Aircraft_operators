CREATE TABLE Aircraft_Operators (
    aircraft_id INT PRIMARY KEY NOT NULL,
    manufacturer_name VARCHAR(100) NOT NULL,
    operator_name VARCHAR(100) NOT NULL,
    model VARCHAR(100) NOT NULL,
    last_maintenance_date DATE,
    maintenance_status ENUM('maintenance_due', 'under_maintenance', 'ready', 'not_needed') NOT NULL,
    FOREIGN KEY (manufacturer_name) REFERENCES Manufacturers(manufacturer_name),
    FOREIGN KEY (operator_name) REFERENCES Operators(operator_name)
);
